package accessmodifier.game;

public class ProtectedWarrior extends ProtectedCommoner{
	public void hunt() {
		System.out.println("전사가 사망하였습니다.");
		hp = hp-2;
		exp = exp + 10;
	}
	
	public void doubleAttack() {
		System.out.println("전사가 더블어택을 실행하였습니다.");
		hp = hp+2;
		exp = exp +10;
	}
	
	public void getAlldata() {
		System.out.println();
		hp = hp + 2;
		exp = exp + 10;
	}
	
}
