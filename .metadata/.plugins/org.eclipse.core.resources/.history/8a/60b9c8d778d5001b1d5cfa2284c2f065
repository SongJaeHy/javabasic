package polymorphism;

public class Job extends Person{
	private String name1;
	
	
	public Job(String name, int age, String name1) {
		super(name1, age);
		this.name1 = name1;
	}
	
	public void showJob() {
		showPerson();
		System.out.println("이름 :" + name1);
		System.out.println("------------");
	}
	
	
}
