package accessmodifier.game;

public class Commoner {
	private int hp=20;
	private int mana=10;
	private int level=1;
	private int exp=0;
	
	public Commoner() {
		this.hp = hp;
		this.mana = mana;
		this.level = level;
		this.exp = exp;
	}
	
	
	public void getter() {
		System.out.println("체력 : " + hp);
		System.out.println("마나 : " + mana);
		System.out.println("레벨 : " + level);
		System.out.println("경험치 : " + exp);
	}
	
}
